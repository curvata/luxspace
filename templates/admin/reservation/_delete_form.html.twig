<form method="post" action="{{ path('reservation.delete', {'id': reservation.id}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ reservation.id) }}">
    <button class="btn_delete">Supprimer</button>
</form>

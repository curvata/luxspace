<form method="post" action="{{ path('returned.delete', {'id': returned.id}) }}" onsubmit="return confirm('Êtes-vous sûr de bien vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ returned.id) }}">
    <button class="btn_delete">Supprimer</button>
</form>
